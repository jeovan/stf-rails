<h1>Documentos#index</h1>
<p>Find me in app/views/documentos/index.html.erb</p>



<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">NOME</th>
      <th>ação</th>
    </tr>
  </thead>
  <tbody>
  <%@documentos.each do |documento|%>
    <tr>
      <th scope="row"><%= documento['id']%></th>
      <td><%= documento['nome']%></td>
      <td>
        <a href="<%= documento_download_path(documento['id'],documento['nome'])%>">baixar arquivo</a>
      </td>
      <td>
        <a href="<%= documento_delete_path(documento['id'])%>">deletar arquivo</a>
      </td>
    </tr>
    <%end%>
  </tbody>
</table>

<a href="/documentos/new">NOVO DOCUMENTO</a>